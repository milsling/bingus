DATABASE_URL="postgresql://user:password@host:5432/database"
SUPABASE_DATABASE_URL="postgresql://user:password@host:5432/database"

# Supabase Configuration - Required for OAuth authentication (Apple, Google)
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"

# Session secret for secure cookies
SESSION_SECRET="your-random-session-secret-here"

# xAI Grok Voice Agent API
# Get your API key from: https://console.x.ai
XAI_API_KEY=your-xai-api-key-here

# LiveKit Configuration for Voice Chat
# Get these from: https://cloud.livekit.io
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# OpenAI (optional, if you want to use OpenAI instead of xAI)
# OPENAI_API_KEY=your-key-here
